{
  "name": "fast-pwa",
  "version": "0.0.1",
  "description": "A demo pwa built with FAST",
  "main": "index.js",
  "scripts": {
    "dev": "vite --open",
    "dev-task": "vite",
    "build": "tsc && vite build",
    "start": "npm run build && vite preview",
    "start-remote": "vite --host"
  },
  "author": "Stephane Comeau",
  "license": "ISC",
  "dependencies": {
    "@pwabuilder/pwainstall": "^1.6.7",
    "@microsoft/fast-element":"^1.10.1",
    "@microsoft/fast-foundation": "^2.46.2",
    "@microsoft/fast-router": "^0.4.1",
    "@microsoft/fast-web-utilities": "latest",
    "@fluentui/web-components": "^2.5.0",
    "workbox-build": "^6.5.2",
    "workbox-core": "^6.5.2",
    "workbox-precaching": "^6.5.2"
  },
  "devDependencies": {
    "typescript": "^4.6.3",
    "vite": "^2.9.0",
    "vite-plugin-pwa": "^0.11.13"
  },
  "prettier": {
    "tabWidth": 2,
    "useTabs": false,
    "semi": true,
    "singleQuote": true,
    "quoteProps": "consistent",
    "trailingComma": "es5",
    "endOfLine": "crlf",
    "bracketSpacing": true
  }
}
