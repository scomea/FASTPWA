    "@microsoft/fast-element": "2.0.0-beta.24",
    "@microsoft/fast-foundation": "3.0.0-alpha.28",
    "@microsoft/fast-router": "1.0.0-alpha.24",
    "@microsoft/fast-web-utilities": "6.0.0",
    "@pwabuilder/pwainstall": "^1.6.7",
